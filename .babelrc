{
  "presets": [
    "next/babel",
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-spread",
    [
      "styled-components",
      {
        "ssr": true
      }
    ]
  ]
}
